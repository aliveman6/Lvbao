.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 1%;
    left: 0;
    z-index: 999;
    width: 94%;
    height: 130rpx;
    box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.1);
    border-radius: 40rpx;
    border: 0px solid blue;
    margin: auto 20rpx;
}
.tabbar_box.iphoneX-height{
    padding-bottom: 66rpx;
}
.middle-wrapper{
  position: absolute;
  right: 310rpx;
  bottom: 0;
  background-color: rgb(121, 27, 27);
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border-top: 2rpx solid #f2f2f3;
}
.middle-wrapper.iphoneX-height{
  bottom: 66rpx;
}
.tabbar_nav{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    height: 100%;
    padding:10rpx 0px 10rpx 0px;
    position: relative;
}

.tabbar_nav .text{
  font-size: 26rpx;
  margin-top: 6rpx;
  font-family: KaiTi;
}

.tabbar_icon{
    height: 60rpx;
    padding-bottom: 5rpx;
}
.special-wrapper{
  position: absolute;
  top: -36rpx;
  width: 96rpx;
  height: 96rpx;
  border-radius: 35%;
  border-top: 2rpx solid #f2f2f3;
  background-color: #fff;
  text-align: center;
  box-sizing: border-box;
  padding: 6rpx;
}
.special-wrapper .tabbar_icon{
    width: 85rpx;
    height: 85rpx;
}
.special-text-wrapper{
  width: 56rpx;
  height: 56rpx;
}